{
  "name": "@dme6/rhubarb",
  "version": "1.0.6",
  "description": "Create desktop programs with web technologies.",
  "scripts": {
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "dev:server": "tsc -w -p ./src/server",
    "dev:client": "tsc -w -p ./src/client",
    "build": "npm run clean && tsc -p ./src/server && tsc -p ./src/client",
    "start": "node ./tests/cpuInfo/index",
    "clean": "rimraf ./dist"
  },
  "keywords": [
    "rbubarb"
  ],
  "author": "dme6",
  "license": "MIT",
  "repository": "dme6/rhubarb",
  "devDependencies": {
    "concurrently": "^6.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/mime": "^2.0.3",
    "@types/node": "^16.3.1",
    "@types/ws": "^7.4.6",
    "mime": "^2.5.2",
    "ws": "^7.5.3"
  }
}
